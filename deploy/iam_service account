eksctl create iamserviceaccount \
--name ekscost-writerecords-sa \
--namespace ekscost \
--cluster eks-dev \
--attach-policy-arn arn:aws:iam::$ACCOUT_ID:policy/EKSCostWriteRecordsPolicy \
--approve \
--override-existing-serviceaccounts

eksctl create iamserviceaccount \
    --name ekscost-dashboard-sa \
    --namespace ekscost \
    --cluster eks-dev \
    --attach-policy-arn arn:aws:iam::$ACCOUT_ID:policy/EKSCostDashboardPolicy \
    --approve \
    --override-existing-serviceaccounts